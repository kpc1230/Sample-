<h1 class="dialog-title">$i18n.getText('session.complete.title')</h1>
#if (!$session)
    #set($session = $action.sessionUI)
#end
<form id="bf-complete-session-form" class="aui">
    <input type="hidden" name="testSessionId" value="$session.getId()"/>
    <div class="form-body bfcom-main-form">
        #parse("templates/excalibur/web/testsessions/test-session-complete-common.vm")
    </div>
    <div class="buttons-container form-footer">
        <div class="buttons">
            <input id="bonfire-complete-button" type="button" class="aui-button aui-button-primary" value="Complete session">
            <a href="#close" class="cancel">Close</a>
        </div>
    </div>
</form>